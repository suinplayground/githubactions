{
  "name": "@suin/semantic-release",
  "version": "0.0.0-development",
  "description": "semantic-release preset",
  "keywords": [
    "semantic-release",
    "preset"
  ],
  "homepage": "https://github.com/suin/semantic-release#readme",
  "bugs": {
    "url": "https://github.com/suin/semantic-release/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/suin/semantic-release.git"
  },
  "license": "MIT",
  "author": "suin",
  "type": "commonjs",
  "exports": {
    ".": {
      "require": "./index.js",
      "types": "./index.d.ts"
    },
    "./changelog.config.js": {
      "require": "./changelog.config.js"
    },
    "./commitlint.config.js": {
      "require": "./commitlint.config.js"
    },
    "./release.config.js": {
      "require": "./release.config.js"
    },
    "./*": {
      "require": "./*.js",
      "types": "./*.d.ts"
    }
  },
  "main": "./index.js",
  "types": "./index.d.ts",
  "files": [
    "*.{js,ts,map}",
    "**/*.{js,ts,map}",
    "dependabot.yml",
    "!*.{test,spec}.*",
    "!testUtils.*"
  ],
  "scripts": {
    "typecheck": "tsc --noEmit",
    "build": "tsc",
    "lint": "echo nothing to do",
    "test": "echo nothing to do",
    "release": "echo nothing to do"
  },
  "dependencies": {
    "@commitlint/types": "^16.0.0",
    "@types/node": "^17.0.16",
    "@types/semantic-release": "^17.2.3",
    "cosmiconfig": "^7.0.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^16.1.0",
    "@commitlint/config-conventional": "^16.0.0",
    "@semantic-release/changelog": "^6.0.1",
    "@semantic-release/exec": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "@suin/semantic-release-yarn": "^1.1.0",
    "@suin/tsconfig": "^0.0.0",
    "conventional-changelog-conventionalcommits": "^4.6.3",
    "git-cz": "^4.8.0",
    "husky": "^7.0.4",
    "prettier": "^2.5.1",
    "semantic-release": "^19.0.2",
    "typescript": "^4.5.5"
  },
  "peerDependencies": {
    "@commitlint/cli": "^16.1.0",
    "@semantic-release/changelog": "^6.0.1",
    "@semantic-release/exec": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "@suin/semantic-release-yarn": "^1.0.2",
    "conventional-changelog-conventionalcommits": "^4.6.3",
    "git-cz": "^4.8.0",
    "semantic-release": "^19.0.2"
  },
  "engines": {
    "node": ">=16.9"
  },
  "publishConfig": {
    "access": "public"
  },
  "packageManager": "yarn@3.1.1"
}
